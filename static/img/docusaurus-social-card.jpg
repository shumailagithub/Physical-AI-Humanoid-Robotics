<?xml version="1.0" encoding="UTF-8"?>
<svg width="1200" height="630" viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <style>
    .title { font-family: Arial, sans-serif; font-size: 48px; font-weight: bold; fill: #234C6A; }
    .subtitle { font-family: Arial, sans-serif; font-size: 28px; fill: #1B3C53; }
    .label { font-family: Arial, sans-serif; font-size: 20px; fill: #456882; }
  </style>
  
  <!-- Background -->
  <rect width="1200" height="630" fill="#E3E3E3"/>
  
  <!-- Physics and AI Elements -->
  <circle cx="600" cy="300" r="150" fill="#234C6A" opacity="0.1"/>
  <circle cx="600" cy="300" r="120" fill="#234C6A" opacity="0.2"/>
  
  <!-- Atom structure -->
  <circle cx="600" cy="300" r="60" fill="#234C6A"/>
  <circle cx="600" cy="200" r="20" fill="#456882" transform="rotate(0 600 300)"/>
  <circle cx="600" cy="200" r="20" fill="#456882" transform="rotate(120 600 300)"/>
  <circle cx="600" cy="200" r="20" fill="#456882" transform="rotate(240 600 300)"/>
  
  <!-- Neural network -->
  <circle cx="450" cy="200" r="18" fill="#1B3C53"/>
  <circle cx="450" cy="350" r="18" fill="#1B3C53"/>
  <circle cx="750" cy="200" r="18" fill="#1B3C53"/>
  <circle cx="750" cy="350" r="18" fill="#1B3C53"/>
  <circle cx="600" cy="150" r="18" fill="#1B3C53"/>
  <circle cx="600" cy="450" r="18" fill="#1B3C53"/>
  
  <!-- Neural connections -->
  <line x1="450" y1="200" x2="450" y2="350" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  <line x1="450" y1="200" x2="750" y2="200" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  <line x1="450" y1="200" x2="750" y2="350" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  <line x1="450" y1="200" x2="600" y2="150" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  <line x1="450" y1="200" x2="600" y2="450" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  
  <line x1="450" y1="350" x2="750" y2="200" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  <line x1="450" y1="350" x2="750" y2="350" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  <line x1="450" y1="350" x2="600" y2="150" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  <line x1="450" y1="350" x2="600" y2="450" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  
  <line x1="750" y1="200" x2="750" y2="350" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  <line x1="750" y1="200" x2="600" y2="150" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  <line x1="750" y1="200" x2="600" y2="450" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  
  <line x1="750" y1="350" x2="600" y2="150" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  <line x1="750" y1="350" x2="600" y2="450" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  
  <line x1="600" y1="150" x2="600" y2="450" stroke="#1B3C53" stroke-width="4" opacity="0.6"/>
  
  <!-- Title Text -->
  <text x="50%" y="100" text-anchor="middle" class="title">Physical-AI Book</text>
  <text x="50%" y="140" text-anchor="middle" class="subtitle">Bridging Physics and Artificial Intelligence</text>
  
  <!-- Labels -->
  <text x="100" y="550" class="label">Physics Concepts</text>
  <text x="500" y="550" class="label">AI Integration</text>
  <text x="900" y="550" class="label">Practical Applications</text>
</svg>